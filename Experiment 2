#include <stdio.h>
int main(){
    int a = 10, b = 4, res;
    res = a + b;
    printf("a + b is %d\n", res);
    res = a - b;
    printf("a - b is %d\n", res);
    res = a * b;
    printf("a * b is %d\n", res);
    res = a / b;
    printf("a / b is %d\n", res);
    res = a % b;
    printf("a %% b is %d\n", res);
    return 0;
}
